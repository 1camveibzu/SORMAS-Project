<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle">
            <stroke android:width="1dp" android:color="@color/warningButtonPressed" />
            <!--<solid android:color="@color/warningButtonPressed"/>-->
            <corners android:radius="@dimen/defaultButtonRadius" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <stroke android:width="1dp" android:color="@color/warningButtonFocused" />
            <!--<solid android:color="@color/warningButtonFocused"/>-->
            <corners android:radius="@dimen/defaultButtonRadius" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="1dp" android:color="@color/warningButton" />
            <!--<solid android:color="@color/warningButton"/>-->
            <corners android:radius="@dimen/defaultButtonRadius" />
        </shape>
    </item>
</selector>